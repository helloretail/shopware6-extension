{% block sw_sales_channel_detail_base %}
    <span v-if="isHelloRetail">
        <helret-sales-channel-hello-retail
            :isLoading="isLoading"
            :salesChannel="salesChannel"
            @invalid-file-name="(v) => this.$emit('invalid-file-name', v)"
            @valid-file-name="(v) => this.$emit('valid-file-name', v)">
        </helret-sales-channel-hello-retail>
    </span>
    <span v-else>
        {% parent %}
    </span>
{% endblock %}
