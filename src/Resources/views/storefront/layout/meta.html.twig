{% sw_extends "@Storefront/storefront/layout/meta.html.twig" %}

{% block layout_head_javascript_csrf %}
    {{ parent() }}
    {% block hello_retail_javascript %}
        <script type="text/javascript">
            (function () {
                window['ADDWISH_PARTNER_ID'] = "{{ config("HelretHelloRetail").config.partnerId }}"
            })();
            (function () {
                var aws = document.createElement('script');
                aws.type = 'text/javascript';
                if (typeof (aws.async) != "undefined") {
                    aws.async = true;
                }
                aws.src = 'https://d1pna5l3xsntoj.cloudfront.net/scripts/company/awAddGift.js#{{ config("HelretHelloRetail").config.partnerId }}';
                var s = document.getElementsByTagName('script')[0];
                s.parentNode.insertBefore(aws, s);
            })();
        </script>
    {% endblock %}
{% endblock %}
